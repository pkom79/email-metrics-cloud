# Public client (browser)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Server-only (used by API routes)
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_JWT_SECRET=

# Site URL (used in emails/links)
NEXT_PUBLIC_SITE_URL=

# Storage config (optional overrides)
# New (preferred)
DATA_INGEST_BUCKET=preauth-uploads
# Public alias (only if client code needs it)
NEXT_PUBLIC_DATA_INGEST_BUCKET=preauth-uploads
# Deprecated (fallback still supported):
# PREAUTH_BUCKET=preauth-uploads
SIGNED_UPLOAD_TTL_SECONDS=3600

# Benchmark feature flag (set to 1 to enable 5-tier benchmarks with Excellent tier)
NEXT_PUBLIC_BENCHMARKS_V2=0

# Postmark (notifications)
POSTMARK_SERVER_TOKEN=
POSTMARK_FROM=
POSTMARK_TEMPLATE_NOTIFICATION_ID=
# Optional stream name
POSTMARK_STREAM=outbound

# Cleanup retention (days)
DIAG_RETENTION_DAYS=90
OUTBOX_RETENTION_DAYS=30
